{
  "name": "crossword-composer",
  "version": "1.0.0",
  "description": "A web-based crossword puzzle solver using MiniZinc WebAssembly",
  "type": "module",
  "private": true,
  "scripts": {
    "build": "bun run convert_wordlist.js",
    "dev": "cd web && python3 -m http.server 8000",
    "start": "cd web && python3 -m http.server 8000",
    "download:train": "bun run download-dataset.js train.csv",
    "download:valid": "bun run download-dataset.js valid.csv",
    "download": "npm run download:train && npm run download:valid"
  },
  "devDependencies": {
    "@types/bun": "latest"
  },
  "peerDependencies": {
    "typescript": "^5"
  },
  "dependencies": {
    "minizinc": "^4.4.4"
  },
  "keywords": [
    "crossword",
    "puzzle",
    "solver",
    "minizinc",
    "constraint-programming",
    "webassembly"
  ],
  "author": "",
  "license": "MPL-2.0"
}
