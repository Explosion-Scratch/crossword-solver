{
  "name": "crossword-composer",
  "version": "1.0.0",
  "description": "A web-based crossword puzzle solver using MiniZinc WebAssembly",
  "type": "module",
  "private": true,
  "scripts": {
    "build": "bun run convert_wordlist.js",
    "dev": "bun --watch web/index.html",
    "start": "python -m http.server 8000 -d web"
  },
  "devDependencies": {
    "@types/bun": "latest"
  },
  "peerDependencies": {
    "typescript": "^5"
  },
  "dependencies": {
    "minizinc": "^4.4.4"
  },
  "keywords": [
    "crossword",
    "puzzle",
    "solver",
    "minizinc",
    "constraint-programming",
    "webassembly"
  ],
  "author": "",
  "license": "MPL-2.0"
}